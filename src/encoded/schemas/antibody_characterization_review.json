 {
    "title": "Antibody characterization review",
    "description": "Schema for submitting antibody characterization review data.",
    "id": "/profiles/antibody_characterization_review.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [ "award", "lab", "reviews", "biosample_term_id", "organism", "lane", "status"],
    "identifyingProperties": ["uuid", "aliases"],
    "additionalProperties": false,
    "mixinProperties": [
        { "$ref": "mixins.json#/schema_version" },
        { "$ref": "mixins.json#/uuid" },
        { "$ref": "mixins.json#/attribution" },
        { "$ref": "mixins.json#/submitted" },
        { "$ref": "characterization_review.json#/properties" },
        { "$ref": "mixins.json#/notes" },

    ],
     "dependencies": {
        "biosample_term_id": ["organism", "lane", "status"],
        "organism": ["biosample_term_id", "lane", "status"],
        "lane": ["biosample_term_id", "organism", "status"],
        "status": ["biosample_term_id", "organism", "lane"]
    },
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "reviews": {
            "title": "Antibody characterization",
            "comment": "See antibody_characterization.json for available identifiers.",
            "linkTo": "antibody_characterization"
        },
        "organism": {
            "title": "Organism",
            "comment": "See organism.json for available identifiers.",
            "type": "string",
            "linkTo": "organism"
        },
        "lane": {
            "title": "Lane",
            "type": "integer"
        },
        "status": {
            "title": "Approval status",
            "description": "The current state of the review for an antibody for a particular cell type.",
            "type": "string",
            "default": "PENDING DCC REVIEW",
            "enum" : [
                "COMPLIANT",
                "PENDING DCC REVIEW",
                "WITHDRAWN BY LAB",
                "NOT COMPLIANT"
            ]
        },
    }
}